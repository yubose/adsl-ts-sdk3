{
  "name": "@aitmed/cadl",
  "version": "1.7.156",
  "description": "A package with utilities to manage and process CADL files.",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "browser": {
    "fs": false
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && npm run build:js && npm run build:types",
    "build:stable": "npm run clean && npm run build:js:stable && npm run build:types",
    "build:js": "cross-env NODE_ENV=development rollup -c --silent",
    "build:js:stable": "cross-env NODE_ENV=production rollup -c --silent",
    "build:types": "tsc --emitDeclarationOnly --skipLibCheck",
    "watch": "cross-env NODE_ENV=development NODE_OPTIONS=\"--stack-trace-limit=10000\" rollup -c -w",
    "watch:stable": "cross-env NODE_ENV=production NODE_OPTIONS=\"--stack-trace-limit=10000\" rollup -c -w",
    "start:test": "npm-run-all --parallel serve:test watch ",
    "test": "cross-env TS_NODE_PROJECT=\"tsconfig.test.json\" NODE_ENV=test ts-mocha --extensions ts --file src/__tests__/setupTests.ts --watch --watch-files src src/__tests__/**/*.test.ts",
    "test:file": "cross-env TS_NODE_PROJECT=\"tsconfig.test.json\" NODE_ENV=test ts-mocha --extensions ts --file './src/__tests__/setupTests.ts' --watch --watch-files src",
    "test:builtin": "  cross-env TS_NODE_PROJECT=\"tsconfig.test.json\"  ts-mocha src/__tests__/builtin/**/**.test.ts -w --watch-files 'src/CADL/services/*.ts'",
    "bump-version": "npm version patch -f",
    "minor-version": "npm version minor -f",
    "prepublish:public:stable": "npm run build:stable && npm run minor-version",
    "prepublish:public": "npm run build && npm run bump-version",
    "prepublish:public:hotfix": "npm run build:stable",
    "publish:public": "npm publish -f",
    "publish:public:hotfix": "npm publish -f",
    "publish:public:stable": "npm publish -f",
    "postpublish:public": "bash scripts/pushCode.sh",
    "postpublish:public:hotfix": "bash scripts/pushCode.sh",
    "postpublish:public:stable": "bash scripts/pushCode.sh",
    "build:doc": "gulp typedoc"
  },
  "repository": {
    "type": "git",
    "url": "http://gitlab.aitmed.com/frontend/cadl.git"
  },
  "author": "Raymundo Martinez",
  "license": "ISC",
  "peerDependencies": {
    "@aitmed/ecos-lvl2-sdk": "^1.4.190",
    "@aitmed/protorepo": "^1.0.24",
    "vercel-toast": "^1.6.2"
  },
  "devDependencies": {
    "@aitmed/ecos-lvl2-sdk": "^1.4.260",
